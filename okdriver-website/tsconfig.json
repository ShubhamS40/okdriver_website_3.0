{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "tailwind.config.js", "src/components/Whyokdriver.jsx", "src/components/Header.jsx", "src/components/Header.jsx", "src/components/Header.jsx", "src/components/Hero.jsx", "src/components/Hero.jsx", "src/app/admin/dashboard/page.jsx", "src/app/admin/fleet/[id]/page.jsx", "src/app/admin/drivers/[id]/page.jsx", "src/app/about/ceo/page.jsx", "src/app/about/page.jsx"],
  "exclude": ["node_modules"]
}